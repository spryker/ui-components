{
    "name": "rating",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/rating/src",
    "prefix": "spy",
    "targets": {
        "build": {
            "executor": "@nx/angular:package",
            "outputs": [
                "{workspaceRoot}/dist/{projectRoot}"
            ],
            "options": {
                "project": "libs/rating/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/rating/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/rating/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/rating/jest.config.ts"
            }
        },
        "storybook": {
            "executor": "@storybook/angular:start-storybook",
            "options": {
                "port": 4400,
                "configDir": "libs/rating/.storybook",
                "browserTarget": "rating:build-storybook",
                "compodoc": true,
                "compodocArgs": [
                    "-e",
                    "json",
                    "-d",
                    "dist"
                ],
                "enableProdMode": false,
                "styles": [
                    ".storybook/styles.less"
                ]
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@storybook/angular:build-storybook",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputDir": "dist/storybook/rating",
                "configDir": "libs/rating/.storybook",
                "browserTarget": "rating:build-storybook",
                "compodoc": true,
                "compodocArgs": [
                    "-e",
                    "json",
                    "-d",
                    "dist"
                ],
                "enableProdMode": false,
                "styles": [
                    ".storybook/styles.less"
                ]
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        }
    },
    "implicitDependencies": [
        "styles",
        "locale"
    ],
    "tags": [
        "pkg:primary",
        "level:molecule",
        "type:component"
    ]
}