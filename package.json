{
  "name": "spryker",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "ct": "git-cz",
    "start": "ng serve",
    "build": "run-s build:*",
    "build:themes": "npm run compile:themes",
    "build:libs": "node tools/scripts/build-libs.js",
    "pretest": "npm run clean:dist",
    "test": "ng test",
    "test:ci": "npm run test -- --runInBand",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "preaffected:test": "npm run clean:dist",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "run:all": "nx run-many --all --target",
    "staged": "run-s staged:*",
    "staged:global": "lint-staged",
    "prestaged:libs": "npm run clean:dist",
    "staged:libs": "lerna exec --concurrency 1 --stream -- lint-staged --config ../../lint-staged-libs.config.js",
    "compile:less": "lessc --js",
    "compile:themes": "run-s compile:theme:*",
    "compile:theme:default": "npm run compile:less -- libs/styles/src/lib/themes/default/standalone.less libs/styles/src/lib/themes/default/standalone.css",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist libs/**/dist",
    "pack": "run-s pack:*",
    "pack:files": "node tools/scripts/multi-copy.js LICENSE dist/libs/*",
    "pack:libs": "lerna exec --concurrency 1 --stream -- node ../../tools/scripts/pack-lib.js",
    "prerelease": "npm run pack",
    "release": "run-s release:*",
    "prerelease:lerna": "npm run pack",
    "release:lerna": "lerna publish --yes --conventional-graduate",
    "release:storybook": "firebase deploy --only hosting:storybook --token $FIREBASE_TOKEN",
    "release-tag": "run-s release-tag:*",
    "prerelease-tag:lerna": "npm run pack",
    "release-tag:lerna": "lerna publish --yes --conventional-prerelease --dist-tag $TRAVIS_BRANCH --preid $TRAVIS_BRANCH",
    "release-tag:storybook": "firebase deploy --only hosting:storybook-next --token $FIREBASE_TOKEN",
    "postinstall_noop": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.1",
    "@angular/common": "^9.0.1",
    "@angular/compiler": "^9.0.1",
    "@angular/core": "^9.0.1",
    "@angular/elements": "^9.0.1",
    "@angular/forms": "^9.0.1",
    "@angular/platform-browser": "^9.0.1",
    "@angular/platform-browser-dynamic": "^9.0.1",
    "core-js": "^3.6.4",
    "ng-zorro-antd": "^9.0.0-beta.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.1",
    "@angular-devkit/build-ng-packagr": "^0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.1",
    "@angular/language-service": "^9.0.1",
    "@babel/core": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nrwl/angular": "^9.0.0",
    "@nrwl/jest": "^9.0.0",
    "@nrwl/storybook": "^9.0.0",
    "@nrwl/workspace": "^9.0.0",
    "@orchestrator/ngx-testing": "^3.0.0",
    "@storybook/addon-knobs": "^6.0.0-alpha.11",
    "@storybook/angular": "^6.0.0-alpha.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.0",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.2.1",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "fast-glob": "^3.1.1",
    "firebase-tools": "^7.13.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "lerna": "^3.20.2",
    "less": "^3.10.3",
    "lint-staged": "^10.0.7",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "tsickle": "^0.38.0",
    "tslint": "^5.0.0",
    "typescript": "~3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{jsx?,tsx?,json,s?css,less,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run staged",
      "pre-push": "run-s format:check affected:test affected:lint"
    }
  }
}
